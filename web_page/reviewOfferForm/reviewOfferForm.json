    },
    "differenceInPercentage" : {
      "type" : "expression",
      "value" : [
        "let percentage = ($data.offer.price*100)/$data.employmentPackage.budgetLimit-100;",
        "let retValue = \"\";",
        "if (percentage < 0) {",
        "    retValue = \"Offer price is \"+ percentage*(-1) + \"% under your budget!\";",
        "}else {",
        "    retValue = retValue = \"Offer price is \"+ percentage + \"% over your budget!\";",
        "} ",
        "return retValue;"
      ],
      "exposed" : false
    },
