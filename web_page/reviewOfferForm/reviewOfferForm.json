    "selectedCandidates" : {
      "type" : "expression",
      "value" : [
        "let allCandidates = $data.AllCandidates;",
        "let selectedCandidates = [];",
        "let selectedIds = $data.offer.userIds.map(String);",
        "let size = allCandidates.length;",
        "for (let i=0; i< size; i++) {",
        "     if(selectedIds.includes(allCandidates[i].id)) {",
        "         selectedCandidates.push(allCandidates[i]);",
        "     }",
        "}",
        "return selectedCandidates;"
      ],
