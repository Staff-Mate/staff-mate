    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.offer ){",
        "\treturn {",
        "\t\t//map offer variable to expected task contract input",
        "\t\tofferInput: {",
        "\t\t\tisAccepted: $data.offer.isAccepted !== undefined ? $data.offer.isAccepted : null",
        "\t\t}",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "taskId" : {
      "type" : "urlparameter",
      "value" : [
        "id"
      ],
      "exposed" : false
    },
    "AllCandidates" : {
      "type" : "url",
      "value" : [
        "../API/identity/user?p=0&c=100&f=group_id={{group[0].id}}"
      ],
      "exposed" : false
    },
    "group" : {
      "type" : "url",
      "value" : [
        "../API/identity/group?p=0&c=100&f=name=candidate"
      ],
      "exposed" : false
    },
    "selectedCandidates" : {
      "type" : "expression",
      "value" : [
        "let allCandidates = $data.AllCandidates;",
        "let selectedCandidates = [];",
        "let selectedIds = $data.offer.userIds.map(String);",
        "let size = allCandidates.length;",
        "for (let i=0; i< size; i++) {",
        "     if(selectedIds.includes(allCandidates[i].id)) {",
        "         selectedCandidates.push(allCandidates[i]);",
        "     }",
        "}",
        "return selectedCandidates;"
      ],
      "exposed" : false
    }
  },
