    "submit_errors_list" : {
      "type" : "expression",
      "value" : [
        "if($data.formOutput && $data.formOutput._submitError && $data.formOutput._submitError.explanations){",
        "\tconst liElements = $data.formOutput._submitError.explanations",
        "\t\t.filter(cause => cause !== null)",
        "\t\t.map(cause => \"<li>\" + cause + \"</li>\")",
        "\t\t.join('');",
        "\tif(liElements){",
        "\t\treturn \"<ul>\" + liElements + \"</ul>\";",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "formOutput" : {
      "type" : "expression",
      "value" : [
        "if( $data.employmentPackage && $data.employmentPackage_wantedEmployees && $data.employmentPackage_employmentPackageOffer ){",
        "\t//attach lazy references variables to parent variables",
        "\t$data.employmentPackage.wantedEmployees = $data.employmentPackage_wantedEmployees;",
        "\t$data.employmentPackage.employmentPackageOffer = $data.employmentPackage_employmentPackageOffer;",
        "\treturn {",
        "\t\t//map employmentPackage variable to expected task contract input",
        "\t\temploymentPackageInput: {",
        "\t\t\temployeeNumber: $data.employmentPackage.employeeNumber !== undefined ? $data.employmentPackage.employeeNumber : null,",
        "\t\t\tbudgetLimit: $data.employmentPackage.budgetLimit !== undefined ? $data.employmentPackage.budgetLimit : null,",
        "\t\t\twantedEmployees: $data.employmentPackage.wantedEmployees.map( it => ({",
        "\t\t\t\temployeePosition: it.employeePosition !== undefined ? it.employeePosition : null,",
        "\t\t\t\texperience: it.experience !== undefined ? it.experience : null,",
        "\t\t\t\teducation: it.education !== undefined ? it.education : null,",
        "\t\t\t\tbudget: it.budget !== undefined ? it.budget : null",
        "\t\t\t}))",
        "\t\t},",
        "\t\tchosenEmployeeId: $data.selectedEmployee",
        "\t}",
        "}"
      ],
      "exposed" : false
    },
    "context" : {
      "type" : "url",
      "value" : [
        "../API/bpm/userTask/{{taskId}}/context"
      ],
      "exposed" : false
    },
    "employmentPackage_wantedEmployees" : {
      "type" : "url",
      "value" : [
